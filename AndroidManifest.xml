<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.curriclo.app" >

    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.curriclo.app.Inicial"
            android:label="@string/title_activity_inicial" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.curriclo.app.schools"
            android:label="@string/title_activity_schools" >
        </activity>
        <activity
            android:name="com.curriclo.app.Linguas"
            android:label="@string/title_activity_linguas" >
        </activity>
        <activity
            android:name="com.curriclo.app.Contatos"
            android:label="@string/title_activity_contatos" >
        </activity>
        <activity
            android:name="com.curriclo.app.Projetos"
            android:label="@string/title_activity_projetos" >
        </activity>
        <activity
            android:name="com.curriclo.app.Redes_Sociais"
            android:label="@string/title_activity_redes__sociais" >
        </activity>
        <activity
            android:name="com.curriclo.app.Fontes"
            android:label="@string/title_activity_fontes" >
        </activity>
    </application>

</manifest>
